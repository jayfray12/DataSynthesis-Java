<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatabuiltProductsEntity" table="databuilt_products"
           schema="datasynthesis">
        <id name="databuiltProductsId">
            <column name="DatabuiltProductsID" sql-type="bigint"/>
        </id>
        <property name="productId">
            <column name="ProductID" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="productName">
            <column name="ProductName" sql-type="varchar(99)" length="99" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="varchar(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>