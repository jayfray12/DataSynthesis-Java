<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatabuiltPhonenumbersEntity" table="databuilt_phonenumbers"
           schema="datasynthesis">
        <id name="dataBuiltPhoneNumbersId">
            <column name="DataBuiltPhoneNumbersID" sql-type="bigint"/>
        </id>
        <property name="areaCode">
            <column name="AreaCode" sql-type="varchar(3)" length="3" not-null="true"/>
        </property>
        <property name="phoneNumber">
            <column name="PhoneNumber" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="completePhoneNumber">
            <column name="CompletePhoneNumber" sql-type="varchar(14)" length="14" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataStatusByStatusId_0" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>