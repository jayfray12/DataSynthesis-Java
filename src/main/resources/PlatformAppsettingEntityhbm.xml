<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.PlatformAppsettingEntity" table="platform_appsetting"
           schema="datasynthesis">
        <id name="appSettingsId">
            <column name="AppSettingsID" sql-type="smallint"/>
        </id>
        <property name="appSettingName">
            <column name="AppSettingName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="appSettingValue">
            <column name="AppSettingValue" sql-type="varchar(199)" length="199" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>