<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.MapDelimiteddatamappingheaderEntity"
           table="map_delimiteddatamappingheader" schema="datasynthesis">
        <id name="delimitedDataMappingHeaderId">
            <column name="DelimitedDataMappingHeaderID" sql-type="smallint"/>
        </id>
        <property name="vendorTechnologyName">
            <column name="VendorTechnologyName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="vendorTechnologyDesc">
            <column name="VendorTechnologyDesc" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="vendorTechnologyInternalCode">
            <column name="VendorTechnologyInternalCode" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="vendorId">
            <column name="VendorID" sql-type="int" not-null="true"/>
        </property>
        <property name="applicationId">
            <column name="ApplicationID" sql-type="int" not-null="true"/>
        </property>
        <property name="organizationId">
            <column name="OrganizationID" sql-type="int" not-null="true"/>
        </property>
        <property name="vendorTechnologyVersion">
            <column name="VendorTechnologyVersion" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="createdBy">
            <column name="CreatedBy" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="delimitedDataMappingHeaderGuid">
            <column name="DelimitedDataMappingHeaderGUID" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="delimiterCharacter">
            <column name="DelimiterCharacter" sql-type="varchar(1)" length="1" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>