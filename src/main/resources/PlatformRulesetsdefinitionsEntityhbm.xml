<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.PlatformRulesetsdefinitionsEntity"
           table="platform_rulesetsdefinitions" schema="datasynthesis">
        <id name="rulesetDefinitionsId">
            <column name="RulesetDefinitionsID" sql-type="char(38)" length="38"/>
        </id>
        <property name="rulesetDefinitionName">
            <column name="RulesetDefinitionName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="ruleSetId">
            <column name="RuleSetID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="stepOrderId">
            <column name="StepOrderID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="operationTypeId">
            <column name="OperationTypeID" sql-type="varchar(7)" length="7" not-null="true"/>
        </property>
        <property name="rulesetDefValue">
            <column name="RulesetDefValue" sql-type="char(40)" length="40" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="effectiveDate">
            <column name="EffectiveDate" sql-type="timestamp" not-null="true"/>
        </property>
        <many-to-one name="platformRulesetsByRuleSetId"
                     class="com.datasynthesis.hibernatemappings.PlatformRulesetsEntity">
            <column name="RuleSetID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataOperationtypeByOperationTypeId"
                     class="com.datasynthesis.hibernatemappings.RefdataOperationtypeEntity">
            <column name="OperationTypeID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>