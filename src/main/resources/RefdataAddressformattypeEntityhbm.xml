<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataAddressformattypeEntity" table="refdata_addressformattype"
           schema="datasynthesis">
        <id name="addressFormatTypeId">
            <column name="AddressFormatTypeID" sql-type="smallint"/>
        </id>
        <property name="addressFormatTypeDesc">
            <column name="AddressFormatTypeDesc" sql-type="varchar(35)" length="35" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>