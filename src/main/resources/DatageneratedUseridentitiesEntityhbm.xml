<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatageneratedUseridentitiesEntity"
           table="datagenerated_useridentities" schema="datasynthesis">
        <id name="userIdentitiesId">
            <column name="UserIdentitiesID" sql-type="bigint"/>
        </id>
        <property name="userIdentityValue">
            <column name="UserIdentityValue" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="userDomain">
            <column name="UserDomain" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="additionalAttributes">
            <column name="AdditionalAttributes" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>