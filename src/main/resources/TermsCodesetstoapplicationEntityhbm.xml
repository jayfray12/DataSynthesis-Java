<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.TermsCodesetstoapplicationEntity"
           table="terms_codesetstoapplication" schema="datasynthesis">
        <id name="codeSetToApplicationId">
            <column name="CodeSetToApplicationID" sql-type="bigint"/>
        </id>
        <property name="codeSetsId">
            <column name="CodeSetsID" sql-type="bigint"/>
        </property>
        <property name="domain">
            <column name="Domain" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="industryStd">
            <column name="IndustryStd" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="specificDetails">
            <column name="SpecificDetails" sql-type="varchar(99)" length="99" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="messageTrigger">
            <column name="MessageTrigger" sql-type="varchar(4)" length="4" not-null="true"/>
        </property>
        <property name="organizationId">
            <column name="OrganizationID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="applicationId">
            <column name="ApplicationID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="vendorId">
            <column name="VendorID" sql-type="smallint" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>