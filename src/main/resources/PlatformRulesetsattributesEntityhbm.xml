<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.PlatformRulesetsattributesEntity"
           table="platform_rulesetsattributes" schema="datasynthesis">
        <id name="rulesetAttributeId">
            <column name="RulesetAttributeID" sql-type="bigint"/>
        </id>
        <property name="ruleSetAttributeDesc">
            <column name="RuleSetAttributeDesc" sql-type="varchar(75)" length="75" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>