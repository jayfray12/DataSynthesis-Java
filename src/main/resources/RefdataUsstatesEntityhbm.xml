<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataUsstatesEntity" table="refdata_usstates"
           schema="datasynthesis">
        <id name="stateId">
            <column name="StateID" sql-type="varchar(2)" length="2"/>
        </id>
        <property name="stateDescription">
            <column name="StateDescription" sql-type="varchar(65)" length="65" not-null="true"/>
        </property>
        <property name="lattitude">
            <column name="Lattitude" sql-type="varchar(12)" length="12" not-null="true"/>
        </property>
        <property name="longitude">
            <column name="Longitude" sql-type="varchar(12)" length="12" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>