<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.PlatformDatagenconfigEntity" table="platform_datagenconfig"
           schema="datasynthesis">
        <id name="dataGenConfigId">
            <column name="DataGenConfigID" sql-type="smallint"/>
        </id>
        <property name="dataTypeGenConfigName">
            <column name="DataTypeGenConfigName" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="specialInstructions">
            <column name="SpecialInstructions" sql-type="varchar(99)" length="99" not-null="true"/>
        </property>
        <property name="runQuantity">
            <column name="RunQuantity" sql-type="int" not-null="true"/>
        </property>
        <property name="minuteInterval">
            <column name="MinuteInterval" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>