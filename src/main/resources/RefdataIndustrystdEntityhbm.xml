<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataIndustrystdEntity" table="refdata_industrystd"
           schema="datasynthesis">
        <id name="industryStd">
            <column name="IndustryStd" sql-type="varchar(6)" length="6"/>
        </id>
        <property name="industryStdDesc">
            <column name="IndustryStdDesc" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>