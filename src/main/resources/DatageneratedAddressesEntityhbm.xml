<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatageneratedAddressesEntity" table="datagenerated_addresses"
           schema="datasynthesis">
        <id name="addressId">
            <column name="AddressID" sql-type="bigint"/>
        </id>
        <property name="addressStreet">
            <column name="AddressStreet" sql-type="varchar(99)" length="99" not-null="true"/>
        </property>
        <property name="addressStreet2">
            <column name="AddressStreet2" sql-type="varchar(59)" length="59" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="addressFormatTypeId">
            <column name="AddressFormatTypeId" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataAddressformattypeByAddressFormatTypeId"
                     class="com.datasynthesis.hibernatemappings.RefdataAddressformattypeEntity">
            <column name="AddressFormatTypeId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>