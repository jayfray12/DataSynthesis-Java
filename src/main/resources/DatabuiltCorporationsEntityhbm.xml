<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatabuiltCorporationsEntity" table="databuilt_corporations"
           schema="datasynthesis">
        <id name="dataBuiltCorporationId">
            <column name="DataBuiltCorporationID" sql-type="bigint"/>
        </id>
        <property name="corporationName">
            <column name="CorporationName" sql-type="varchar(99)" length="99" not-null="true"/>
        </property>
        <property name="address1">
            <column name="Address1" sql-type="varchar(99)" length="99" not-null="true"/>
        </property>
        <property name="city">
            <column name="City" sql-type="varchar(70)" length="70" not-null="true"/>
        </property>
        <property name="stateId">
            <column name="StateID" sql-type="varchar(2)" length="2" not-null="true"/>
        </property>
        <property name="zipCode">
            <column name="ZipCode" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="varchar(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>