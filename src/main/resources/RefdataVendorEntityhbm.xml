<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataVendorEntity" table="refdata_vendor"
           schema="datasynthesis">
        <id name="vendorId">
            <column name="VendorID" sql-type="int"/>
        </id>
        <property name="vendorName">
            <column name="VendorName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="vendorGuid">
            <column name="VendorGUID" sql-type="char(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>