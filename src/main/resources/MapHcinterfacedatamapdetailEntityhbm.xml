<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.MapHcinterfacedatamapdetailEntity"
           table="map_hcinterfacedatamapdetail" schema="datasynthesis">
        <id name="hcInterfaceDataMapDetailId">
            <column name="HCInterfaceDataMapDetailID" sql-type="bigint"/>
        </id>
        <property name="interfaceDataMapHeaderId">
            <column name="InterfaceDataMapHeaderID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="sourceApplicationId">
            <column name="SourceApplicationID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="vrsn">
            <column name="Vrsn" sql-type="varchar(7)" length="7" not-null="true"/>
        </property>
        <property name="msgSegment">
            <column name="MsgSegment" sql-type="varchar(3)" length="3" not-null="true"/>
        </property>
        <property name="segmentFieldOrder">
            <column name="SegmentFieldOrder" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="msgFieldNumber">
            <column name="MsgFieldNumber" sql-type="varchar(7)" length="7" not-null="true"/>
        </property>
        <property name="msgFieldName">
            <column name="MsgFieldName" sql-type="varchar(154)" length="154" not-null="true"/>
        </property>
        <property name="msgFieldLength">
            <column name="MsgFieldLength" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="dataType">
            <column name="DataType" sql-type="varchar(5)" length="5" not-null="true"/>
        </property>
        <property name="sensitivityFlagId">
            <column name="SensitivityFlagID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="hcInterfaceDataMapDetailIguid">
            <column name="HCInterfaceDataMapDetailIGUID" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>