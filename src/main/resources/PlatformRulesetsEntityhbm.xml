<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.PlatformRulesetsEntity" table="platform_rulesets"
           schema="datasynthesis">
        <id name="ruleId">
            <column name="RuleID" sql-type="bigint"/>
        </id>
        <property name="ruleName">
            <column name="RuleName" sql-type="varchar(65)" length="65" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="expirationDate">
            <column name="ExpirationDate" sql-type="timestamp" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
        <set name="platformRulesetsdefinitionsByRuleId" inverse="true">
            <key>
                <column name="RuleSetID" not-null="true"/>
            </key>
            <one-to-many not-found="ignore"
                         class="com.datasynthesis.hibernatemappings.PlatformRulesetsdefinitionsEntity"/>
        </set>
    </class>
</hibernate-mapping>