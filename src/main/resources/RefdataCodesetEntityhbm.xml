<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataCodesetEntity" table="refdata_codeset"
           schema="datasynthesis">
        <id name="codeSetsId">
            <column name="CodeSetsID" sql-type="bigint"/>
        </id>
        <property name="codeSetName">
            <column name="CodeSetName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="industryStd">
            <column name="IndustryStd" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="codesetGuid">
            <column name="CodesetGUID" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="fieldMapping">
            <column name="FieldMapping" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>