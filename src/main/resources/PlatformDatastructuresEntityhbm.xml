<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.PlatformDatastructuresEntity" table="platform_datastructures"
           schema="datasynthesis">
        <id name="platformDataStructuresId">
            <column name="PlatformDataStructuresID" sql-type="smallint"/>
        </id>
        <property name="dataStructureName">
            <column name="DataStructureName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="sensitivityFlagId">
            <column name="SensitivityFlagID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="platformDataStructuresGuid">
            <column name="PlatformDataStructuresGUID" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
        <set name="platformDatastructurestodataattributesByPlatformDataStructuresId" inverse="true">
            <key>
                <column name="PlatformDataStructuresID" not-null="true"/>
            </key>
            <one-to-many not-found="ignore"
                         class="com.datasynthesis.hibernatemappings.PlatformDatastructurestodataattributesEntity"/>
        </set>
    </class>
</hibernate-mapping>