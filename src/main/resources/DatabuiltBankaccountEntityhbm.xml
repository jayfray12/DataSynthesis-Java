<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatabuiltBankaccountEntity" table="databuilt_bankaccount"
           schema="datasynthesis">
        <id name="dataBuiltBankAccountsId">
            <column name="DataBuiltBankAccountsID" sql-type="bigint"/>
        </id>
        <property name="bankAccountNumber">
            <column name="BankAccountNumber" sql-type="varchar(12)" length="12" not-null="true"/>
        </property>
        <property name="bankAccountName">
            <column name="BankAccountName" sql-type="varchar(75)" length="75" not-null="true"/>
        </property>
        <property name="bankRoutingId">
            <column name="BankRoutingID" sql-type="varchar(9)" length="9" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="varchar(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>