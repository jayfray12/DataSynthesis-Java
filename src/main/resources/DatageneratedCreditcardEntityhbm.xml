<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatageneratedCreditcardEntity" table="datagenerated_creditcard"
           schema="datasynthesis">
        <id name="creditCardId">
            <column name="CreditCardID" sql-type="bigint"/>
        </id>
        <property name="creditCardNumber">
            <column name="CreditCardNumber" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="creditCardName">
            <column name="CreditCardName" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>