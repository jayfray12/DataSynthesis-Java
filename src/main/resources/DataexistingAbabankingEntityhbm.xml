<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DataexistingAbabankingEntity" table="dataexisting_ababanking"
           schema="datasynthesis">
        <id name="abaBankingId">
            <column name="ABABankingID" sql-type="bigint"/>
        </id>
        <property name="routingNumber">
            <column name="RoutingNumber" sql-type="varchar(9)" length="9" not-null="true"/>
        </property>
        <property name="telegraphicName">
            <column name="TelegraphicName" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="customerName">
            <column name="CustomerName" sql-type="varchar(36)" length="36" not-null="true"/>
        </property>
        <property name="city">
            <column name="City" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="state">
            <column name="State" sql-type="varchar(2)" length="2" not-null="true"/>
        </property>
        <property name="zipCode">
            <column name="ZipCode" sql-type="varchar(5)" length="5" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>