<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DatabuiltNamesEntity" table="databuilt_names"
           schema="datasynthesis">
        <id name="dataBuiltNamesId">
            <column name="DataBuiltNamesID" sql-type="bigint"/>
        </id>
        <property name="completeName">
            <column name="CompleteName" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>