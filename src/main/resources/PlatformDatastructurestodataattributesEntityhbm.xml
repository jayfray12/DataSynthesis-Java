<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.PlatformDatastructurestodataattributesEntity"
           table="platform_datastructurestodataattributes" schema="datasynthesis">
        <id name="platformDataStructuresToDataAttributesId">
            <column name="PlatformDataStructuresToDataAttributesID" sql-type="smallint"/>
        </id>
        <property name="platformDataStructuresId">
            <column name="PlatformDataStructuresID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="compositeDataStructureName">
            <column name="CompositeDataStructureName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="sensitivityFlagId">
            <column name="SensitivityFlagID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="platformDataStructuresToDataAttributesGuid">
            <column name="PlatformDataStructuresToDataAttributesGUID" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="platformDataAttributesId">
            <column name="PlatformDataAttributesID" sql-type="smallint" not-null="true"/>
        </property>
        <many-to-one name="platformDatastructuresByPlatformDataStructuresId"
                     class="com.datasynthesis.hibernatemappings.PlatformDatastructuresEntity">
            <column name="PlatformDataStructuresID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataSensitivityflagBySensitivityFlagId"
                     class="com.datasynthesis.hibernatemappings.RefdataSensitivityflagEntity">
            <column name="SensitivityFlagID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
        <many-to-one name="platformDataattributesByPlatformDataAttributesId"
                     class="com.datasynthesis.hibernatemappings.PlatformDataattributesEntity">
            <column name="PlatformDataAttributesID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>