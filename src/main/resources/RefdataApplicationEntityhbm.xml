<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataApplicationEntity" table="refdata_application"
           schema="datasynthesis">
        <id name="appGuid">
            <column name="AppGUID" sql-type="char(38)" length="38"/>
        </id>
        <property name="applicationCustomCode">
            <column name="ApplicationCustomCode" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="applicationDesc">
            <column name="ApplicationDesc" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>