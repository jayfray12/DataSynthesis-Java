<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.AuditingDatarequestEntity" table="auditing_datarequest"
           schema="datasynthesis">
        <id name="dataRequestId">
            <column name="DataRequestID" sql-type="bigint"/>
        </id>
        <property name="dataRequestDate">
            <column name="DataRequestDate" sql-type="datetime(3)" not-null="true"/>
        </property>
        <property name="createdByUser">
            <column name="CreatedByUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="organizaton">
            <column name="Organizaton" sql-type="bigint" not-null="true"/>
        </property>
        <property name="application">
            <column name="Application" sql-type="bigint" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
        <set name="auditingDatarequestdetailsByDataRequestId" inverse="true">
            <key>
                <column name="DataRequestID" not-null="true"/>
            </key>
            <one-to-many not-found="ignore"
                         class="com.datasynthesis.hibernatemappings.AuditingDatarequestdetailEntity"/>
        </set>
    </class>
</hibernate-mapping>