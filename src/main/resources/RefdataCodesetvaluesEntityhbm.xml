<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataCodesetvaluesEntity" table="refdata_codesetvalues"
           schema="datasynthesis">
        <id name="codeSetValueId">
            <column name="CodeSetValueID" sql-type="bigint"/>
        </id>
        <property name="codesetId">
            <column name="CodesetID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="qtyValue">
            <column name="QtyValue" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="qtyValueDesc">
            <column name="QtyValueDesc" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="applicationId">
            <column name="ApplicationID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <many-to-one name="refdataCodesetByCodesetId" class="com.datasynthesis.hibernatemappings.RefdataCodesetEntity">
            <column name="CodesetID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataApplicationByApplicationId"
                     class="com.datasynthesis.hibernatemappings.RefdataApplicationEntity">
            <column name="ApplicationID" not-null="true"/>
        </many-to-one>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>