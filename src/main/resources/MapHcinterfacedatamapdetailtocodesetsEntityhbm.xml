<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.MapHcinterfacedatamapdetailtocodesetsEntity"
           table="map_hcinterfacedatamapdetailtocodesets" schema="datasynthesis">
        <id name="hcInterfaceDataMapToCodesetsDataMappingDetailId">
            <column name="HCInterfaceDataMapToCodesetsDataMappingDetailID" sql-type="bigint"/>
        </id>
        <property name="hcInterfaceDataMapDetailId">
            <column name="HCInterfaceDataMapDetailID" sql-type="bigint" not-null="true"/>
        </property>
        <property name="codesetsId">
            <column name="CodesetsID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="fieldNumber">
            <column name="FieldNumber" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>