<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.MapHcinterfacedatamapheaderEntity"
           table="map_hcinterfacedatamapheader" schema="datasynthesis">
        <id name="hcInterfaceDataMapHeaderId">
            <column name="HCInterfaceDataMapHeaderID" sql-type="bigint"/>
        </id>
        <property name="interfaceName">
            <column name="InterfaceName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="interfaceDesc">
            <column name="InterfaceDesc" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="vendorId">
            <column name="VendorID" sql-type="int" not-null="true"/>
        </property>
        <property name="applicationId">
            <column name="ApplicationID" sql-type="int" not-null="true"/>
        </property>
        <property name="messageType">
            <column name="MessageType" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="messageTrigger">
            <column name="MessageTrigger" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="messageVersion">
            <column name="MessageVersion" sql-type="varchar(7)" length="7" not-null="true"/>
        </property>
        <property name="organizationId">
            <column name="OrganizationID" sql-type="int" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="hcInterfaceDataMapHeaderGuid">
            <column name="HCInterfaceDataMapHeaderGUID" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="industryStd">
            <column name="IndustryStd" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <many-to-one name="refdataStatusByStatusId" class="com.datasynthesis.hibernatemappings.RefdataStatusEntity">
            <column name="StatusID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>