<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DataexistingUpccodesEntity" table="dataexisting_upccodes"
           schema="datasynthesis">
        <id name="upcCodeId">
            <column name="UPCCodeID" sql-type="bigint"/>
        </id>
        <property name="upcCodeName">
            <column name="UPCCodeName" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="upcProductName">
            <column name="UPCProductName" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>