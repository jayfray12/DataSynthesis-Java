<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.DataexistingZipcodeusEntity" table="dataexisting_zipcodeus"
           schema="datasynthesis">
        <id name="zipCodeId">
            <column name="ZipCodeID" sql-type="bigint"/>
        </id>
        <property name="zipCode">
            <column name="ZipCode" sql-type="char(5)" length="5"/>
        </property>
        <property name="zipCodeType">
            <column name="ZipCodeType" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="city">
            <column name="City" sql-type="varchar(75)" length="75" not-null="true"/>
        </property>
        <property name="state">
            <column name="State" sql-type="varchar(2)" length="2" not-null="true"/>
        </property>
        <property name="lattitude">
            <column name="Lattitude" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="longitude">
            <column name="Longitude" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="location">
            <column name="Location" sql-type="varchar(99)" length="99" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="createdUser">
            <column name="CreatedUser" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="registeredApp">
            <column name="RegisteredApp" sql-type="char(38)" length="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>