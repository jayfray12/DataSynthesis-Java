<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.datasynthesis.hibernatemappings.RefdataOperationtypeEntity" table="refdata_operationtype"
           schema="datasynthesis">
        <id name="operationTypeId">
            <column name="OperationTypeID" sql-type="varchar(7)" length="7"/>
        </id>
        <property name="operationTypeName">
            <column name="OperationTypeName" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="createdDate">
            <column name="CreatedDate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="statusId">
            <column name="StatusID" sql-type="smallint" not-null="true"/>
        </property>
        <set name="platformRulesetsdefinitionsByOperationTypeId" inverse="true">
            <key>
                <column name="OperationTypeID" not-null="true"/>
            </key>
            <one-to-many not-found="ignore"
                         class="com.datasynthesis.hibernatemappings.PlatformRulesetsdefinitionsEntity"/>
        </set>
    </class>
</hibernate-mapping>